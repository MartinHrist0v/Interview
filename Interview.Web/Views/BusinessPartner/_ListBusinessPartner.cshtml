@model ICollection<Interview.Web.Models.BusinessPartnerDetailsView>
@foreach (var item in Model)
{
    
    <div class="col-md-4 business-partner-box " id="@item.Id">
        
        <h4>@item.Name <span class="glyphicon glyphicon-remove" style="float:right"></span></h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(i => item.Name)
                
            </dt>

            <dd>
                @Html.DisplayFor(i => item.Name)
            </dd>

            <dt>
                @Html.DisplayNameFor(i => item.Email)
            </dt>

            <dd>
                @Html.DisplayFor(i => item.Email)
            </dd>

            <dt>
                @Html.DisplayNameFor(i => item.PhoneNumber)
            </dt>

            <dd>
                @Html.DisplayFor(i => item.PhoneNumber)
            </dd>

        </dl>
        <p>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }, new { @class = "btn btn-primary" })
            @Html.ActionLink("Details", "Details", new { id = item.Id }, new { @class = "btn btn-default" })

        </p>
    </div>


}
